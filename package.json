{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "babel-cli": "^6.26.0",
    "events": "^2.0.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "lodash": "^4.17.5",
    "material-ui": "1.0.0-beta.34",
    "material-ui-icons": "^1.0.0-beta.36",
    "prettier": "^1.11.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.1",
    "sift": "^5.1.0",
    "typeface-roboto": "^0.0.54"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "scripts": {
    "precommit": "lint-staged",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "copy-json-rsync": "rsync -avm --include='*.json' -f 'hide,! */' src/mson lib",
    "copy-json": "cd src && find . -name '*.json' -exec cp --parents \\{\\} ../lib \\;",
    "compile": "export NODE_ENV=production && babel src/mson -d lib/mson && npm run copy-json",
    "touch": "find src -exec touch {} \\;"
  }
}
