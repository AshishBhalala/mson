{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^1.0.0",
    "@material-ui/icons": "^1.0.0",
    "apollo-cache-inmemory": "^1.2.1",
    "apollo-client": "^2.3.1",
    "apollo-link-context": "^1.0.8",
    "apollo-link-http": "^1.5.4",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "events": "^2.0.0",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "husky": "^0.14.3",
    "js-cookie": "^2.2.0",
    "lint-staged": "^7.0.0",
    "lodash": "^4.17.5",
    "prettier": "^1.11.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-google-recaptcha": "^0.12.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^1.1.4",
    "sift": "^5.1.0",
    "typeface-roboto": "^0.0.54"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "scripts": {
    "lint": "eslint src",
    "precommit": "lint-staged",
    "test-server-start": "export NODE_ENV=production && ./node_modules/babel-cli/bin/babel-node.js ../mson-server/bin/test-server.js --port=4002 --prefix=mson_test_ --CORE_TEST &",
    "test-server-stop": "pkill -f --signal=SIGINT CORE_TEST & exit 0",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test-set-up": "export NODE_ENV=production && ./node_modules/babel-cli/bin/babel-node.js ./src/test/set-up.js",
    "test-tear-down": "export NODE_ENV=production && ./node_modules/babel-cli/bin/babel-node.js ./src/test/tear-down.js",
    "just-test": "./test.sh",
    "test": "npm run lint && npm run just-test",
    "copy-json-rsync": "rsync -avm --include='*.json' -f 'hide,! */' src/mson lib",
    "copy-json": "cd src && find . -name '*.json' -exec cp --parents \\{\\} ../lib \\;",
    "compile": "export NODE_ENV=production && babel src/mson -d lib/mson && npm run copy-json",
    "touch": "find src -exec touch {} \\;",
    "setup": "cd node_modules && ln -sf ../../mson-server/lib/client mson-client",
    "employees-install": "export NODE_ENV=production && ./node_modules/babel-cli/bin/babel-node.js ./src/employees/install.js",
    "employees-uninstall": "export NODE_ENV=production && ./node_modules/babel-cli/bin/babel-node.js ./src/employees/uninstall.js"
  },
  "devDependencies": {
    "wait-on": "^2.1.0"
  }
}
